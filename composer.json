{
    "name": "laravel-enso/enso",
    "description": "Laravel Enso is a solid start for any Laravel Based project",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "barryvdh/laravel-debugbar": "^3.1",
        "fideloper/proxy": "~4.0",
        "laravel-enso/commentsmanager": "1.3.*",
        "laravel-enso/contacts": "1.3.*",
        "laravel-enso/core": "1.3.*",
        "laravel-enso/dataimport": "1.3.*",
        "laravel-enso/documentsmanager": "1.3.*",
        "laravel-enso/notifications": "1.3.*",
        "laravel-enso/rolemanager": "1.3.*",
        "laravel-enso/vueadminlte": "^1.3",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "^0.8.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.6.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "dev-master",
        "laravel-enso/testhelper": "1.3.*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
